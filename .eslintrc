{
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      1,
      "single"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-unused-vars": [
      1, {
        "vars": "all",
        "args": "after-used"
      }
    ],
    "no-console": 0
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jquery": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module",
		"ecmaFeatures": {
			"jsx": true,
			"experimentalObjectRestSpread": true,
    	"modules": true
  	}
	}
}
